# Configuração do Sankhya MCP Server
# Copie este arquivo para .env e preencha com seus dados

# URL base do Sankhya (sem barra no final)
SANKHYA_BASE_URL=https://api.sankhya.com.br

# Token de autenticação (Bearer token)
SANKHYA_TOKEN=seu_token_aqui

# App Key da aplicação
SANKHYA_APP_KEY=sua_app_key_aqui
